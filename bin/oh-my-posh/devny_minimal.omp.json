{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#f0ff50",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": " \uf07b {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#35ccee",
          "properties": {
            "branch_icon": ""
          },
          "style": "diamond",
          "template": " \ue725 {{ .HEAD }}",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#f1184c",
          "style": "plain",
          "template": " \u279c ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
